{
  "project": {
    "name": "financial-intelligence-platform",
    "version": "1.0.0",
    "environment": "development"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 5000
  },
  "database": {
    "type": "postgresql",
    "migrations": {
      "directory": "migrations",
      "tablePrefix": "cursor_"
    }
  },
  "ml_models": {
    "save_directory": "models/saved",
    "cache_directory": "models/cache"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/cursor_migration.log"
  }
}
